{
    "QMainWindow": [
        {
            "tittle": "App",
            "frameless": true,
            "translucentBg": true,
            "shadow": [
                {
                    "color" : "#fff",
                    "blurRadius" : 20,
                    "xOffset" : 20,
                    "centralWidget" : "centralWidget"
                }
            ],
            "navigation": [
                {
                    "minimize": "minimize",
                    "close": "close",
                    "restore": [
                        {
                            "buttonName" : "maximize"
                        }
                    ],
                    "moveWindow": "header_menu"
                }
            ]
        }
    ],
    "QSettings": [
        {
            "ThemeSettings": [
                {
                    "CustomTheme": []
                }
            ],
            "AppSettings": {
                "OriginizationName": "App",
                "ApplicationName": "App",
                "OriginizationDomain": "App"
            }
        }
    ],
    "QCustomSlideMenu": [
        {
            "name": "side_menu",
            "defaultsize": [
                {
                    "width": 45,
                    "height": "parent"
                }
            ],
            "collapsedSize": [
                {
                    "width": 45,
                    "height": "parent"
                }
            ],
            "expandedSize": [
                {
                    "width": 150,
                    "height": "parent"
                }
            ],
            "toggleButton": [
                {
                    "buttonName": "menu"
                }
            ]
        }
    ],
    "menuTransitionAnimation": [
        {
            "animationDuration": 10,
            "animationEasingCurve": "OutBack"
        }
    ],
	"QStackedWidget":[
		{
            "name": "pages",
            "transitionAnimation":[
				{
					"fade":[
						{
							"active": true,
							"duration":1000,
							"easingCurve": "OutBack"
						}
					],
					"slide":[
						{
							"active":true,
							"duration": 1000,
							"direction": "horizontal",
							"easingCurve": "OutBack"
						}
					]
				}
			],
			"navigation":[
				{
					"navigationButtons":[
						{
                            "btn_home": "central",
                            "btn_cliente" : "cliente",
                            "btn_fornecedor": "fornecedor" 
						}
					]
				}
			]
	
		}
	]
}